<div class="row">
    {{#each notes}}
    <div class="col-md-3">
        <div class="card">
            <div class="card-body">
               <h4 class="card-title d-flex justify-content-between align-items-center align-items-center">
                   {{title}}<a href="/notes/edit/{{_id}}"><i class="fas fa-edit"></i></a>
                   </h4>
               <p>{{description}}</p>   

            </div>
        </div>
    </div>
</div>

